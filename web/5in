"gfs5" using 23 with lp title "gfs v1 - limited area",\
"acnfs5" using 23 with lp title "acnfs v2 - limited area",\
"gfs5" using 13 with lp title "gfs v1 - global",\
"acnfs5" using 13 with lp title "acnfs v2 - global"
