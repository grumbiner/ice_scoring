set term png
set grid
set pointsize 2
set xlabel "Day 5 forecasts 4/20 to 5/09"
#
set out "bias.png"
set ylabel "bias"
#set yrange [0.0:0.05]
plot \
"gfs5" using 14 with lp title "gfs v1 - limited area",\
"acnfs5" using 14 with lp title "acnfs v2 - limited area",\
"gfs5" using 4 with lp title "gfs v1 - global",\
"acnfs5" using 4 with lp title "acnfs v2 - global"
#
set out "correct.png"
set ylabel "correct"
#set yrange [0.85:1.05]
